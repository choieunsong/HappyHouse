CA,IAAA,KAAA,CACA,GADA,CACA,4CADA,EAEA;AACA;AACA;AACA;AALA,KAMA,IANA,CAMA,gBAAA;AAAA,UAAA,IAAA,QAAA,IAAA;AACA;AACA,MAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,IAAA;AACA,KAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,KAAA,CAAA,SAAA,CAAA;AACA,KAdA;AAgBA,IAAA,KAAA,CACA,GADA,CACA,wDADA,EAEA,IAFA,CAEA,iBAAA;AAAA,UAAA,IAAA,SAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,IAAA,CAAA,QAAA,EAAA,IAAA,CAAA,MAAA;AACA,KAPA,EAQA,KARA,CAQA,YAAA;AACA,MAAA,KAAA,CAAA,eAAA,CAAA;AACA,KAVA;AAWA;AAlDA,CAAA","sourcesContent":["<template>\r\n  <div style=\"margin-top: 40px;\">\r\n    <div class=\"container\">\r\n\r\n      <div class=\"text-right\" style=\"margin: 30px 0 30px;\">\r\n        <router-link to=\"/create\" class=\"btn btn-primary\">등록하기</router-link>\r\n      </div>\r\n\r\n      <div class=\"table-responsive\" v-if=\"boards.length > 0\">\r\n\r\n        <table class=\"table table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th>번호</th>\r\n              <th>작성자</th>\r\n              <th>제목</th>\r\n              <th>날짜</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr scope=\"row\" v-for=\"(board, index) in boards\" :key=\"index\"  @click=\"searchBoard(board.no )\">\r\n              <td>{{ board.no }}</td>\r\n              <td v-text=\"board.writer\"></td>\r\n              <td v-text=\"board.title\"></td>\r\n              <td>{{ board.regtime | toDate }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div v-else>게시글이 없습니다.</div>\r\n    </div>\r\n\r\n  </div> \r\n  \r\n</template>\r\n\r\n<script>\r\n// import http from '@/util/http-common.js'\r\nimport moment from \"moment\";\r\nimport axios from \"axios\";\r\nimport {bus} from '@/boardbus'\r\nexport default {\r\n  name: \"list\",\r\n  data() {\r\n    return { boards: [] };\r\n  },\r\n  filters: {\r\n    toDate: function (regtime) {\r\n      return moment(new Date(regtime)).format(\"YYYY.MM.DD\");\r\n    }\r\n  },\r\n  methods: {\r\n    searchBoard(no){\r\n      this.$router.push('/search')\r\n      setTimeout(()=>{\r\n        bus.$emit('searchBoard', no)\r\n      }, 100);\r\n\r\n    },\r\n    movePage() {\r\n      this.$router.push(\"/create\");\r\n    }\r\n  },\r\n  created() {\r\n    axios\r\n      .get(\"http://localhost:9090/happyhouse/qna/board\")\r\n      // .then(result => {\r\n      //   console.log(result);\r\n      //   console.log(result.data);\r\n      // })\r\n      .then(({ data }) => {\r\n        //결과 Object에서 data만 추출\r\n        this.boards = data;\r\n        console.log(\"list......\", data);\r\n      })\r\n      .catch(err => {\r\n        console.log(err);\r\n        alert(\"오류 발생!!\");\r\n      });\r\n\r\n    axios\r\n      .get(\"http://localhost:9090/happyhouse/qna/board/getUserInfo\")\r\n      .then(({data}) => {\r\n        console.log(data);\r\n        console.log(data.username);\r\n        console.log(data.userno);\r\n        this.$store.dispatch('setUserInfo', data.username, data.userno);\r\n      })\r\n      .catch(() => {\r\n        alert('로그인정보 불러오기 실패');\r\n      });\r\n  }\r\n  \r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}